{
  "manifest_version": 2,
  "name": "XXNBL_CICO_BROWSER_RFRSH",
  "description": "Refreshes browser window after period of inactivity",
  "version": "1.0",
  "browser_action": {
    "default_icon": "stonksGuy.png"
  },
  "permissions": ["*://kiosk.niagarawater.com/*","*://dev-kiosk.niagarawater.com/*"],
  "content_scripts": [
    {
      "matches": ["*://kiosk.niagarawater.com/*","*://dev-kiosk.niagarawater.com/*"],
      "js": ["content_script.js"]
    }
  ]
}
